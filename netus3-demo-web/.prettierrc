{
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 130,
  "overrides": [
    {
      "files": ".prettierrc",
      "options": { "parser": "json" }
    }
  ],
  "semi": false,
  "tabWidth": 2,
  "useTabs": true,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "auto",
  "plugins": ["prettier-plugin-tailwindcss"]
}
